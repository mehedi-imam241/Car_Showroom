<AnchorPane prefHeight="650.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Admin.AddUserController">
   <children>
      <ImageView fitHeight="650.0" fitWidth="1000.0" opacity="0.2">
         <image>
            <Image url="@../../image/Background.jpg" />
