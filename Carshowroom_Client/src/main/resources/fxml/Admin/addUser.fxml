      </ImageView>
      <Text layoutX="418.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add User">
         <font>
            <Font size="40.0" />
         </font>
      </Text>
      <TextField fx:id="nameTxt" layoutX="418.0" layoutY="200.0" promptText="Name">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordTxt" layoutX="418.0" layoutY="274.0" promptText="Password">
         <font>
            <Font size="24.0" />
         </font>
      </PasswordField>
      <Button fx:id="register" layoutX="440.0" layoutY="388.0" mnemonicParsing="false" onAction="#onPressedRegister" text="Register">
         <font>
            <Font size="24.0" />
