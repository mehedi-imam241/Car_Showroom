<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<AnchorPane prefHeight="650.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Admin.allUserController">
   <children>
       <ImageView fitHeight="650.0" fitWidth="1000.0" opacity="0.2">
           <image>
               <Image url="@../../image/Background.jpg" />
           </image>
       </ImageView>
      <Text layoutX="450.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="All Users">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Button fx:id="toBack" layoutX="487.0" layoutY="560.0" mnemonicParsing="false" onAction="#onPressedToBack" text="&lt;&lt; Back" />
      <TableView fx:id="tableView" layoutX="185.0" layoutY="262.0" prefHeight="272.0" prefWidth="673.0" AnchorPane.leftAnchor="185.0">
        <columns>
          <TableColumn fx:id="userNameColumn" prefWidth="338.0" resizable="false" text="User Name" />
          <TableColumn fx:id="passwordColumn" minWidth="0.0" prefWidth="334.0" text="Password" />
        </columns>
      </TableView>
      <Button fx:id="loadAllUser" layoutX="363.0" layoutY="147.0" mnemonicParsing="false" onAction="#onPressedLoadAllUser" prefHeight="77.0" prefWidth="311.0" text="Load all users">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
