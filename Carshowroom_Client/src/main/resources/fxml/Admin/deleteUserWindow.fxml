      </Text>
      <TextField fx:id="userName" layoutX="479.0" layoutY="266.0" prefHeight="58.0" prefWidth="276.0" promptText="Name">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <Button fx:id="delete" layoutX="449.0" layoutY="358.0" mnemonicParsing="false" onAction="#onPressedDelete" text="Delete">
         <font>
            <Font size="24.0" />
