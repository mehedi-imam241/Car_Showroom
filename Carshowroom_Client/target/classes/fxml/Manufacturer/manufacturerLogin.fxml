            <Font size="18.0" />
         </font></PasswordField>
      <Text layoutX="14.0" layoutY="206.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User Name :">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Text layoutX="24.0" layoutY="254.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password  :">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
